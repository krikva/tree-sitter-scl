{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "scl",
      "camelcase": "Scl",
      "title": "tree-sitter-scl",
      "scope": "source.scl",
      "file-types": [
        "scl"
      ],
      "injection-regex": "^scl$",
      "class-name": "TreeSitterScl",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "locals": "queries/locals.scm",
      "indents": "queries/indents.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree sitter parser for siemens scl",
    "authors": [
      {
        "name": "krikva"
      }
    ],
    "links": {
      "repository": "https://github.com/krikva/tree-sitter-scl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}